								},
								{
									"Type": "NodeCodeBlockFenceInfoMarker",
									"CodeBlockInfo": "Z2xzbA==",
									"Properties": {
										"id": ""
									}
								},
								{
									"Type": "NodeCodeBlockCode",
									"Data": "float2 FaceSpaceUV(float2 uv,float3 viewDir,float4 ZPos)\n                {\n\n                    viewDir.z /= ZPos.x;\n\n                    //约束UV回归0~1\n                    float2 newUV = frac(uv);\n\n                    //UV，0～1，转换为表面空间，X，Y为 -1~1，Z为1\n                    float3 FaceUVPos = float3(newUV * 2.0 - 1.0, 1.0);\n\n                    //使用视角朝向，获取物体自身空间坐标\n                    float3 FaceViewSpace = 1.0 / viewDir;\n\n                    //单位空间尺寸-平面深度空间尺寸\n                    float3 k = abs(FaceViewSpace) - FaceUVPos * FaceViewSpace;\n\n                    //获得最小坐标值\n                    float kMin = min(min(k.x, k.y), k.z);\n\n                    //表面坐标累加最小坐标缩放单位空间坐标\n                    float3 FaceInteriorSpace = FaceUVPos + kMin * viewDir;\n\n                    //获取缩放符号，来确定是否左右镜像。\n                    FaceInteriorSpace.x *=lerp(-1,1,step(0, 1 / ZPos.y));\n\n                    //使用缩放的绝对值去乘UV，使用加法累加偏移\n                    return (FaceInteriorSpace.xy*0.5+0.5)* abs(1 / ZPos.y)+ ZPos.zw;\n                }\n",
									"Properties": {
										"id": ""
									}
								},
								{
									"Type": "NodeCodeBlockFenceCloseMarker",
									"Data": "```",
									"CodeBlockFenceLen": 3,
									"Properties": {
										"id": ""
									}
								}
							]
						}
